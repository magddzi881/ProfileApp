datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model Furniture {
  id       Int     @id @default(autoincrement())
  name     String
  category String
  price    Int
  inStock  Boolean
}
